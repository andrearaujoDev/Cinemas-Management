        <servlet-class>servlets.Sessoes.SvCad_Edit_Sessao</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SvDeleteSessao</servlet-name>
        <servlet-class>servlets.Sessoes.SvDeleteSessao</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SvCad_Edit_Funcionario</servlet-name>
        <url-pattern>/SvCad_Edit_Funcionario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SvDeleteFuncionario</servlet-name>
        <url-pattern>/SvDeleteFuncionario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SvCad_Edit_Caixa</servlet-name>
        <url-pattern>/SvCad_Edit_Caixa</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SvDeleteCaixa</servlet-name>
        <url-pattern>/SvDeleteCaixa</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SvCad_Edit_Consumo</servlet-name>
        <url-pattern>/SvCad_Edit_Consumo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SvDeleteConsumo</servlet-name>
        <url-pattern>/SvDeleteConsumo</url-pattern>
