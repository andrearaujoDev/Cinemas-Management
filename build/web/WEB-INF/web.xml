    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SvDeleteSessao</servlet-name>
        <url-pattern>/SvDeleteSessao</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    
    <error-page>
        <error-code>500</error-code>
        <location>/internalservererror.jsp</location>
    </error-page>
    
    
