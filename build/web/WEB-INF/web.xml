    </servlet>
    <servlet>
        <servlet-name>SvDeleteFuncionario</servlet-name>
        <servlet-class>servlets.Funcionarios.SvDeleteFuncionario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SvCad_Edit_Caixa</servlet-name>
        <servlet-class>servlets.Caixas.SvCad_Edit_Caixa</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SvDeleteCaixa</servlet-name>
        <servlet-class>servlets.Caixas.SvDeleteCaixa</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SvCad_Edit_Consumo</servlet-name>
        <servlet-class>servlets.Consumos.SvCad_Edit_Consumo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SvDeleteConsumo</servlet-name>
        <servlet-class>servlets.Consumos.SvDeleteConsumo</servlet-class>
